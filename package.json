{
  "name": "mazes101",
  "version": "0.0.0-development",
  "description": "Utilities to generate, render and hack with any sort of mazes",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "type": "module",
  "files": [
    "cjs/**/!(*.tsbuildinfo)",
    "esm/**/!(*.tsbuildinfo)",
    "types/**/!(*.tsbuildinfo)",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "compile": "ttsc -b ./src/tsconfig.cjs.json ./src/tsconfig.esm.json ./src/tsconfig.types.json",
    "compile:deno": "node tools/build-deno.js",
    "watch": "ttsc --watch",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nmanumr/mazes101.git"
  },
  "keywords": [
    "maze",
    "maze-puzzle",
    "circular-maze",
    "rectangular-maze",
    "game"
  ],
  "author": "nmanumr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nmanumr/mazes101/issues"
  },
  "homepage": "https://nmanumr.github.io/mazes101/",
  "exports": {
    ".": {
      "node": "./cjs/index.js",
      "default": "./esm/index.js"
    },
    "./boards": {
      "node": "./cjs/boards/index.js",
      "default": "./esm/boards/index.js"
    },
    "./generators": {
      "node": "./cjs/generators/index.js",
      "default": "./esm/generators/index.js"
    },
    "./masks": {
      "node": "./cjs/masks/index.js",
      "default": "./esm/masks/index.js"
    },
    "./renderers": {
      "node": "./cjs/renderers/index.js",
      "default": "./esm/renderers/index.js"
    },
    "./*": {
      "node": "./cjs/*.js",
      "default": "./esm/*.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "preact": "^10.5.14",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.4.2",
    "ts-transformer-keys": "^0.4.3",
    "tslib": "^2.2.0",
    "tsutils": "^3.21.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4"
  },
  "dependencies": {}
}
